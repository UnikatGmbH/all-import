{
    "name": "unikat/all-import",
    "description": "All Import should help migrating data from one database to another",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Konstantin Knopp",
            "email": "k.knopp@das-unikat.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "maatwebsite/excel": "^2.1",
        "intervention/image": "^2.4",
        "league/csv": "^9.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "orchestra/testbench": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Unikat\\AllImport\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Unikat\\AllImport\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Unikat\\AllImport\\AllImportServiceProvider"
            ]
        }
    }
}
